package whitepaper.smcall.alarm;

public class AlarmStructure {
	
	// 시간
	public int			time_hour;
	public int			time_minute;
	
	// 반복설정
	public boolean[]	repeat = new boolean[7];
	
	// 매치조건  //이성만, 시간범위
	public boolean		match_only_opposite;
	public boolean		match_range;
	
	// 모닝콜 알림 타입
	public boolean		type_sound;
	public int			type_sound_volume;
	public boolean		type_vibe;
	
	// 활성 유무
	public boolean		alive;
	
	
	public AlarmStructure(){
		
	}
}
